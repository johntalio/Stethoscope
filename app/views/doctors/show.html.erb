<p><%= image_tag @doctor.image_url %></p>
<p><%= @doctor.first_name %> <%= @doctor.last_name %></p>
<p><%= @doctor.title %></p>
<% if @doctor.bio.empty? %>
  <p> This doctor did not provide a bio. </p>
<% else %>
  <p><%= @doctor.bio %></p>
<% end %>

<% if logged_in? %>
  <%= button_to "Save doctor's profile", {:controller => "records", :action => "create", :doctor_id => @doctor.id, :user_id=> session[:user_id]} , :method=>:post  %>
<% end %>
